{
  "presets": [["next/babel"]],
  "env": {
    "development": {
      "presets": [
        [
          "next/babel",
          {
            "styled-jsx": {
              "plugins": [
                [
                  "styled-jsx-plugin-sass",
                  {
                    "sassOptions": {
                      "data": "@import './styles/vars/_colors.scss'; @import './styles/vars/_sizes.scss';"
                    }
                  }
                ],
                "styled-jsx-plugin-postcss"
              ]
            }
          }
        ]
      ]
    },
    "production": {
      "presets": [
        [
          "next/babel",
          {
            "styled-jsx": {
              "plugins": [
                [
                  "styled-jsx-plugin-sass",
                  {
                    "sassOptions": {
                      "data": "@import './styles/vars/_colors.scss'; @import './styles/vars/_sizes.scss';"
                    }
                  }
                ],
                "styled-jsx-plugin-postcss"
              ]
            }
          }
        ]
      ]
    },
    "test": {
      "presets": [
        [
          "next/babel",
          {
            "styled-jsx": {
              "babel-test": true,
              "plugins": [
                [
                  "styled-jsx-plugin-sass",
                  {
                    "sassOptions": {
                      "data": "@import './styles/vars/_colors.scss'; @import './styles/vars/_sizes.scss';"
                    }
                  }
                ]
              ]
            }
          }
        ]
      ]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": { "@": "." }
      }
    ]
  ]
}
